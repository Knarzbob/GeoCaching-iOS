      <groundspeak:name>AL#17	Waldkreuzung   // Magische Kreise II</groundspeak:name>
      <groundspeak:placed_by>black_dog2402</groundspeak:placed_by>
      <groundspeak:owner id="11367851">black_dog2402</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
