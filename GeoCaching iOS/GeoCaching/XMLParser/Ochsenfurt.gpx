          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="9474808">Rienis</groundspeak:finder>
          <groundspeak:text encoded="False">Mit der ganzen Familie (Polo6N2000 + Reußeckers) unterwegs gewesen und diese Dose geloggt. Dfdc</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="573597968">
