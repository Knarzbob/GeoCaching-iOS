    <type>Geocache|Multi-cache</type>
    <groundspeak:cache id="1707793" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Moon Path</groundspeak:name>
      <groundspeak:placed_by>A_&amp;_C</groundspeak:placed_by>
